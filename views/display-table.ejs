<% if(db_rows_array.length>0) { %>
<table border="1">
<tr>
<th> Admin Id </th>
<th> Name </th>
<th> Email </th>
<th> Password </th>
<th> Action </th>
</tr>
<% for(var i=0; i<db_rows_array.length;i++) { %>
<tr>
<td><%= db_rows_array[i].admin_id %></td>
<td><%= db_rows_array[i].admin_name %></td>
<td><%= db_rows_array[i].admin_email %></td>
<td><%= db_rows_array[i].admin_password %></td>
<td>
    <a href='/edit/<%= db_rows_array[i].admin_id %>'>Edit</a> |
    <a href='/delete/<%= db_rows_array[i].admin_id %>'>Delete</a> |
    <a href='/show/<%= db_rows_array[i].admin_id %>'>Show</a>
</td>
</tr>   
<% } %>
</table>
<% } else { %>
    No Record Found
<% } %>